pipeline {
    agent any 
    stages {
        stage ("welcome") {
            steps {
                script {
                for (i=1;i<=10;i++){
                    println "My i value is ${i}"
                }

                list1=[10,20,30,40,50]
                for (i in list1) {
                    println "my i value is ${i}"
                }
                 }
                }
            }
        }
    }
}
